{
  "name": "express-serverless-typescript",
  "version": "1.0.0",
  "description": "Boilerplate for running ExpressJS+TypeScript in AWS Lambda",
  "license": "MIT",
  "scripts": {
    "dev": "NODE_ENV=development sls offline start --stage offline",
    "log": "sls logs -f app -t",
    "deploy-prod": "NODE_ENV=production sls deploy --stage prod",
    "deploy-prod-lambda": "sls deploy function -f app --stage prod",
    "test": "NODE_ENV=test jest --coverage --forceExit -w 1 --detectOpenHandles"
  },
  "dependencies": {
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "express-jwt": "^6.0.0",
    "express-validator": "^6.11.1",
    "knex": "^0.95.6",
    "morgan": "^1.10.0",
    "mysql2": "^2.2.5",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^6.6.2",
    "sequelize-typescript": "^2.1.0",
    "serverless-http": "^2.7.0"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.10",
    "@types/express": "^4.17.12",
    "@types/express-jwt": "^6.0.1",
    "@types/jest": "^26.0.23",
    "@types/morgan": "^1.9.2",
    "@types/node": "^15.12.1",
    "@types/supertest": "^2.0.11",
    "@types/validator": "^13.1.3",
    "jest": "^27.0.4",
    "jest-html-reporters": "^2.1.6",
    "prettier": "^2.3.1",
    "serverless": "^2.44.0",
    "serverless-bundle": "^4.3.1",
    "serverless-dotenv-plugin": "^3.9.0",
    "serverless-offline": "^6.9.0",
    "supertest": "^6.1.3",
    "ts-jest": "^27.0.3",
    "ts-node": "^10.0.0",
    "typescript": "^4.3.2"
  }
}
